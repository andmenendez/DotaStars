<div class= "DotaStarsNavBar">
  <div class = "DotaStarsContainer">
    <div class= "DotaStarsLogo"> 
      <%= link_to "DotaStars",     events_path %> 
    </div>
    <div class= "DotaStarsNav">
      <div class="Nav_Tab"> <%= link_to "Events",   events_path %>  </div>
      <div class="Nav_Tab"> <%= link_to "Matches",  matches_path %> </div>
      <div class="Nav_Tab"> <%= link_to "Teams",    teams_path %>   </div>
      <!-- <div class="Nav_Tab"> <%= link_to "Players",  players_path %> </div> -->
    </div>
    <div class="DotaStarsNavLogin">
      <%= link_to "Log In", '/' %> |
      <%= link_to "Create Account", '/' %>
    </div>
  </div>
</div>
 
<div class="flash_container">
  <% if flash[:success] %>
    <div class="flash-success"><%= flash[:success] %></div>
  <% end %>
    <% if flash[:alert] %>
    <div class="flash-danger"><%= flash[:alert] %></div>
  <% end %>
</div>